{
  "name": "mapbrowser",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "axios": "^0.19.0",
    "office-ui-fabric-react": "^7.23.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "json-server": "^0.15.0",
    "http-proxy-middleware": "^0.19.1",
    "react-test-renderer": "^16.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-mockbackend": "concurrently  \"NODE_ENV=development react-scripts start\" \"node util/mock-json-server/server.js\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
